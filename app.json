{
  "expo": {
    "name": "shakeMe",
    "slug": "shakeMe-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.shakeapp.expo.googlesingin",
      "config":{
        "googleSignIn":{
          "reservedClientId":"com.googleusercontent.apps.167067609377-uova9oje9oddrbmqvuruqgkaele3la2k",
        }
      }
    },
    "android":{
      "package":"com.shakeapp.expo.googlesingin",
      "googleServiceFile": "./google-services.json"
    }
  }
}